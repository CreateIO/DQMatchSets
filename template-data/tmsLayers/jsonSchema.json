{
  "$schema": "http://json-schema.org/draft-04/schema#",
  "type": "object",
  "properties": {
    "tmsLayers": {
      "type": "object",
      "properties": {},
      "patternProperties": {
        ".*": {
          "type": "object",
          "properties": {
            "tmsProvider": { "type": "string" },
            "tmsLayer": { "type": "string" },
            "tmsLayerName": { "type": "string" },
            "url": { "type": "string" },
            "extension": { "type": "string" },
            "coords": {
              "type": "object",
              "properties": {
                "xMin": { "type": "number" },
                "yMin": { "type": "number" },
                "xMax": { "type": "number" },
                "xMax": { "type": "number" }
              },
              "required": [
                "xMin",
                "yMin",
                "xMax",
                "yMax"
              ]
            },
            "tileWidth": { "type": "number" },
            "tileHeight": { "type": "number" },
            "minimumLevel": { "type": "number" },
            "maximumLevel": { "type": "number" },
            "alpha": { "type": "number" },
            "flag": { "type": "boolean" }
          },
          "additionalProperties": false
        }
      }
    }
  },
  "required": [
    "tmsLayers"
  ]
}
