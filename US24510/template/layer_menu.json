                            "minElevation": 9000,
                            "maxElevation": 10000,
                            "minScale": 1.0,
                            "maxScale": 0.0
                        },
                        "scale": 1.0
                    }
                }
            },
            "polygons": {
                "exists": true,
                "polygonLayers": {
                    "polygon1": {
                        "type": "colorHash",
                        "name": "entity.properties.NAME",
                        "color": {
                            "type": "random",
                            "alpha": 0.5,
                            "assignments": {
                                "color1": {
                                    "field": "HR",
                                    "hexValue": "#FFAAD4"
                                },
                                "color2": {
                                    "field": "DD-HPA",
                                    "hexValue": "#A55D61"
                                },
                                "color3": {
                                    "field": "DD-HPB",
                                    "hexValue": "#A55D61"
                                },
                                "color4": {
                                    "field": "DD-HPC",
                                    "hexValue": "#A55D61"
                                }
                            }
                        },
                        "material": "new Cesium.ColorMaterialProperty(color)",
                        "outlineColor": "Cesium.Color.WHITE",
                        "outlineWidth": 7.0
                    }
                }
            },
